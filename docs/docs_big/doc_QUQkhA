    <title>Quadruply clad fiber</title>
    <ns>0</ns>
    <id>41612</id>
    <revision>
      <id>529984231</id>
      <parentid>529976223</parentid>
      <timestamp>2012-12-27T14:33:35Z</timestamp>
      <contributor>
        <username>Srleffler</username>

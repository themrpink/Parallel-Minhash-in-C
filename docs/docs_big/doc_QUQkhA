    <title>Pulse duration</title>
    <ns>0</ns>
    <id>41605</id>
    <revision>
      <id>890591587</id>
      <parentid>879550795</parentid>
      <timestamp>2019-04-02T08:46:47Z</timestamp>
      <contributor>
        <username>John of Reading</username>

    <title>Maximum usable frequency</title>
    <ns>0</ns>
    <id>41359</id>
    <revision>
      <id>961618934</id>
      <parentid>960874551</parentid>
      <timestamp>2020-06-09T14:07:51Z</timestamp>
      <contributor>
        <username>Kvng</username>

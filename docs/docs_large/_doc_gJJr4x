    <title>Substitution method</title>
    <ns>0</ns>
    <id>41757</id>
    <revision>
      <id>915542551</id>
      <parentid>904637604</parentid>
      <timestamp>2019-09-13T21:56:19Z</timestamp>
      <contributor>
        <username>Bender235</username>

    <title>Improved-definition television</title>
    <ns>0</ns>
    <id>41254</id>
    <revision>
      <id>563153225</id>
      <parentid>476472331</parentid>
      <timestamp>2013-07-06T20:23:53Z</timestamp>
      <contributor>
        <username>BD2412</username>

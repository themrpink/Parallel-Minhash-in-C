{{Speciesbox
| image = Micropterus_punctulatus.jpg
| status = LC
| status_system = IUCN3.1
| status_ref = &lt;ref name=&quot;iucn&quot;&gt;{{Cite iucn | author =NatureServe | year = 2013 | title = ''Micropterus punctulatus''  | page = e.T202565A18234277 | doi = 10.2305/IUCN.UK.2013-1.RLTS.T202565A18234277.en | accessdate = 21 May 2020}}&lt;/ref&gt;
| taxon = Micropterus punctulatus

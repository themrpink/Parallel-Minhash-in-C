    <title>Paired disparity code</title>
    <ns>0</ns>
    <id>41482</id>
    <revision>
      <id>833325331</id>
      <parentid>664321958</parentid>
      <timestamp>2018-03-30T21:42:31Z</timestamp>
      <contributor>
        <username>Comp.arch</username>
